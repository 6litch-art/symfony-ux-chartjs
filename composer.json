{
    "name": "glitchr/ux-chartjs",
    "type": "symfony-bundle",
    "description": "Chart.js integration for Symfony",
    "keywords": ["symfony", "ux", "chart", "chart.js", "chartjs"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyerconde@gmail.com"
        },
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Symfony\\UX\\Chartjs\\": "."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Symfony\\UX\\Chartjs\\": "./Tests"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/framework-bundle": "^4.0|^5.0|^6.0",
        "symfony/dependency-injection": "^4.0|^5.0|^6.0",
        "symfony/http-kernel": "^4.0|^5.0|^6.0",
        "symfony/config": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/framework-bundle": "^4.0|^5.0|^6.0",
        "symfony/twig-bundle": "^4.0|^5.0|^6.0",
        "symfony/var-dumper": "^4.0|^5.0|^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
